    </Method>
    <Method Name="M_EvaluateInlineStatus" Id="{3ab8a915-7979-05ce-1a8d-dd2933ba9d34}">
      <Declaration><![CDATA[METHOD M_EvaluateInlineStatus
VAR
	nForIndex : UDINT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[FOR nForIndex := GC.nNumMinMovers TO GVL_General.nNumMovers DO
	
	stTrackStatus.SumMoverCount := stTrackStatus.SumMoverCount + TrackSectors[nForIndex].SingularSectorStatus[GC.OUTER_MERGE_ALL].MoverCountOnSector;
	
	IF stTrackStatus.SumMoverCount <> 0 THEN
		stTrackStatus.MergePermIntlk := FALSE;
		EXIT;
	ELSIF stTrackStatus.SumMoverCount = 0 THEN
		stTrackStatus.MergePermIntlk := TRUE;
		stTrackStatus.SumMoverCount := 0;
	END_IF
	
//	IF NOT TrackSectors[nForIndex].SingularSectorStatus[GC.OUTER_MERGE_CLEAR].MoverIsOnSector THEN
//		stTrackStatus.MergePermIntlk[GC.MERGE_PERMINTLK_MID] := TRUE;
//	ELSE
//		stTrackStatus.MergePermIntlk[GC.MERGE_PERMINTLK_MID] := FALSE;
//	END_IF

//	IF NOT TrackSectors[nForIndex].SingularSectorStatus[GC.OUTER_MERGE_COL_AVOID].MoverIsOnSector THEN
//		stTrackStatus.MergePermIntlk[GC.MERGE_PERMINTLK_OUTCURV] := TRUE;
//	ELSE
//		stTrackStatus.MergePermIntlk[GC.MERGE_PERMINTLK_OUTCURV] := FALSE;
//	END_IF

END_FOR]]></ST>
      </Implementation>
    </Method>
